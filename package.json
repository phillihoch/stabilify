{
  "name": "stabilify",
  "version": "0.1.3",
  "description": "AI-powered auto-healing for Playwright tests",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Philipp Hoch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillihoch/stabilify.git"
  },
  "bugs": {
    "url": "https://github.com/phillihoch/stabilify/issues"
  },
  "homepage": "https://github.com/phillihoch/stabilify#readme",
  "engines": {
    "node": ">=24"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./reporter": {
      "types": "./dist/reporter.d.ts",
      "import": "./dist/reporter.mjs",
      "require": "./dist/reporter.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "dev": "rollup -c -w",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "playwright",
    "testing",
    "auto-healing",
    "ai"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
